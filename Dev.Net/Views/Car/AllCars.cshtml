
@{
    ViewData["Title"] = "AllCars";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>All Cars</h1>
<div class="text-center">
    <table class="table table-dark">
        <tr>
            <th>Car model </th>
            <th>Car Color</th>
          

        </tr>
        @foreach (Car elm in ViewBag.currentcar)
        {

    <tr>
        <td> @elm.Model </td>
        <td>@elm.Color</td>
        

    </tr>

        }
    </table>
</div>

